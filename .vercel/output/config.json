{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/about/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/comments/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/comments/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/comments/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/reactions/?$",
			"dest": "_render"
		},
		{
			"src": "^/notes/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/notes/?$",
			"dest": "_render"
		},
		{
			"src": "^/open-source-contributions/?$",
			"dest": "_render"
		},
		{
			"src": "^/reach/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}